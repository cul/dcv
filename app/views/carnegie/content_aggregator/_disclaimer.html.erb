<% document ||= {} -%>
<% asset ||= {} -%>
<% if document['physical_description_digital_origin_ssm']&.detect { |val| val.downcase == 'digitized microfilm'} %>
<div class="child-content text-muted"><em>Documents scanned from microfilm may have variable contrast quality.</em></div>
<% end -%>
<% if has_closed_children? %>
<div class="child-content text-muted"><%= show_repository_location_with_contact(document: document, field: ActiveFedora::SolrService.solr_name('lib_repo_full', :symbol, type: :string)).sub("This item is accessible", "Additional content may be accessible").html_safe %></div>
<% end -%>
