<!DOCTYPE html>
<html lang="en" class="no-js">
	<head>
		<%= render partial: 'shared/head_includes' %>
		<% # dcv css is always pulled in via head_includes %>
		<%= stylesheet_link_tag subsite_layout unless subsite_layout == 'dcv' %>
		<% # dcv js is always pulled in via head_includes %>
		<%= javascript_include_tag subsite_layout unless subsite_layout == 'dcv' %>
		<meta name="robots" content="noindex" />
	</head>
	<body class="empty <%= render_subsite_body_classes %> <%= params[:action] %>" style="padding-top: 50px;">
		<%= render :partial => 'shared/topnavbar' %>
		<div id="outer-wrapper" class="container">
			<div class="row">
				<div id="main-container" class="col-md-12">
					<%= yield %>
				</div>
			<%= render :partial => 'footer' %>
			</div><!--/.row-->
		</div><!--/#outer-wrapper-->
		<%= render :partial => 'shared/footer_includes' %>
	</body>
</html>
